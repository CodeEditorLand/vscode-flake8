{
	"activationEvents": [
		"onLanguage:python",
		"workspaceContains:.flake8",
		"workspaceContains:*.py"
	],
	"author": {
		"email": "Land@PlayForm.Cloud",
		"name": "üåÜ‚ÄÅ‚Äî‚ÄÅLand‚ÄÅ‚Äî",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandFlake8/issues"
	},
	"capabilities": {
		"virtualWorkspaces": {
			"description": "Virtual Workspaces are not supported with flake8.",
			"supported": false
		}
	},
	"contributes": {
		"commands": [
			{
				"category": "Flake8",
				"command": "flake8.restart",
				"title": "%command.restartServer%"
			}
		],
		"configuration": {
			"properties": {
				"flake8.args": {
					"default": [],
					"items": {
						"type": "string"
					},
					"markdownDescription": "%settings.args.description%",
					"scope": "resource",
					"type": "array"
				},
				"flake8.cwd": {
					"default": "${workspaceFolder}",
					"examples": [
						"${workspaceFolder}/src",
						"${fileDirname}"
					],
					"markdownDescription": "%settings.cwd.description%",
					"scope": "resource",
					"type": "string"
				},
				"flake8.enabled": {
					"default": true,
					"markdownDescription": "%settings.enabled.description%",
					"scope": "resource",
					"type": "boolean"
				},
				"flake8.ignorePatterns": {
					"default": [],
					"examples": [
						[
							"**/site-packages/**/*.py",
							".vscode/*.py"
						]
					],
					"items": {
						"type": "string"
					},
					"markdownDescription": "%settings.ignorePatterns.description%",
					"scope": "resource",
					"type": "array",
					"uniqueItems": true
				},
				"flake8.importStrategy": {
					"default": "useBundled",
					"enum": [
						"useBundled",
						"fromEnvironment"
					],
					"enumDescriptions": [
						"%settings.importStrategy.useBundled.description%",
						"%settings.importStrategy.fromEnvironment.description%"
					],
					"markdownDescription": "%settings.importStrategy.description%",
					"scope": "window",
					"type": "string"
				},
				"flake8.interpreter": {
					"default": [],
					"items": {
						"type": "string"
					},
					"markdownDescription": "%settings.interpreter.description%",
					"scope": "resource",
					"type": "array"
				},
				"flake8.path": {
					"default": [],
					"examples": [
						[
							"~/global_env/flake8"
						],
						[
							"~/.env/python",
							"-m",
							"flake8"
						]
					],
					"items": {
						"type": "string"
					},
					"markdownDescription": "%settings.path.description%",
					"scope": "resource",
					"type": "array"
				},
				"flake8.severity": {
					"additionalProperties": {
						"enum": [
							"Error",
							"Hint",
							"Information",
							"Warning"
						],
						"type": "string"
					},
					"default": {
						"E": "Error",
						"F": "Error",
						"I": "Information",
						"W": "Warning"
					},
					"markdownDescription": "%settings.severity.description%",
					"scope": "resource",
					"type": "object"
				},
				"flake8.showNotifications": {
					"default": "off",
					"enum": [
						"off",
						"onError",
						"onWarning",
						"always"
					],
					"enumDescriptions": [
						"%settings.showNotifications.off.description%",
						"%settings.showNotifications.onError.description%",
						"%settings.showNotifications.onWarning.description%",
						"%settings.showNotifications.always.description%"
					],
					"markdownDescription": "%settings.showNotifications.description%",
					"scope": "machine",
					"type": "string"
				}
			}
		},
		"languages": [
			{
				"filenames": [
					".flake8"
				],
				"id": "ini"
			}
		]
	},
	"dependencies": {
		"fs-extra": "11.2.0"
	},
	"description": "%extension.description%",
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/fs-extra": "11.0.4",
		"@types/glob": "8.1.0",
		"@types/node": "20.x",
		"glob": "10.3.12",
		"ts-loader": "9.5.1"
	},
	"displayName": "Flake8",
	"extensionDependencies": [
		"ms-python.python"
	],
	"galleryBanner": {
		"color": "#1e415e",
		"theme": "dark"
	},
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandFlake8#readme",
	"icon": "icon.png",
	"keywords": [
		"land"
	],
	"license": "SEE LICENSE IN LICENSE",
	"main": "./dist/extension.js",
	"name": "flake8",
	"preview": true,
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandFlake8.git"
	},
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"compile": "webpack",
		"compile-tests": "tsc -p . --outDir out",
		"prepublishOnly": "Build 'Source/**/*.ts'",
		"vsce-package": "vsce package -o flake8.vsix",
		"watch-tests": "tsc -p . -w --outDir out"
	},
	"serverInfo": {
		"module": "flake8",
		"name": "Flake8"
	},
	"type": "module",
	"version": "0.0.1"
}
